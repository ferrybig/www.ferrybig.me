{
	"target": "esnext",
    "module": "ESNext",
	"compilerOptions": {
		"strict": true,
		"jsx": "react-jsx",
		"jsxImportSource": "../jsx",
		"esModuleInterop": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
		"downlevelIteration": true,
		"lib": ["es6"]
	}
}
