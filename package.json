{
  "name": "www.ferrybig.me-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n NextJS,Markdown -c green,blue --kill-others \"npm run --silent dev-frontend\" \"cd md-compiler && npm --silent run dev\"",
    "dev-frontend": "next dev -p 2999",
    "build": "next build",
    "build-via-docker": "rm -fr out && docker build -t www.ferrybig.me:files --target export . -o out",
    "start": "serve out -p 2999",
    "lint": "next lint",
    "tsc": "tsc --noEmit",
    "clean": "find app -name '*.js' -delete && rm -rf out md-compiler/out md-compiler/.tsbuildinfo app/.gitignore app/content.d.ts"
  },
  "dependencies": {
    "@giscus/react": "^3.0.0",
    "classnames": "^2.5.1",
    "feed": "^4.2.2",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/web-app-manifest": "^1.0.8",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/typescript-estree": "^7.8.0",
    "acorn": "^8.11.3",
    "acorn-jsx": "^5.3.2",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-react": "^7.34.0",
    "postcss": "^8.4.35",
    "postcss-preset-env": "^9.4.0",
    "serve": "^14.2.3",
    "typescript": "^5.4.5"
  },
  "browserslist": [
    "> 0.2% and not dead"
  ]
}
